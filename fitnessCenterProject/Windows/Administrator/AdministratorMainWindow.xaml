<Window x:Class="fitnessCenterProject.Windows.AdministratorMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fitnessCenterProject.Windows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Background="LightGray"
        Title="Administrator" Height="550" Width="800">
    <Grid>
        <Button Background="gray" Content="LogOut" HorizontalAlignment="Left" Margin="261,431,0,0" VerticalAlignment="Top" Width="100" Height="33" Click="logOutButton"/>
        <Button Background="gray" Content="Exit" HorizontalAlignment="Left" Margin="429,431,0,0" VerticalAlignment="Top" Width="100" Height="33" Click="exitTheApp"/>
        <Label FontWeight="Bold" FontSize="20" Content="Administrator" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="315,34,0,0"/>
        <Separator HorizontalAlignment="Left" Height="36" Margin="91,71,0,0" VerticalAlignment="Top" Width="626"/>
        <DataGrid AutoGeneratedColumns="changeVisibilityAdmin" BorderBrush="Black" SelectedItem="{Binding Path=selectedAdmin}" AutoGenerateColumns="True" IsReadOnly="True" Name="adminDataInformationDataGrid" HorizontalAlignment="Left" Height="52" VerticalAlignment="Top" Width="527" Margin="134,165,0,0"/>
        <Label FontWeight="Bold" FontSize="16" Content="User data" HorizontalAlignment="Left" Margin="343,112,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.529,1.073"/>
        <Button Background="Gray" Content="Change data" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="343,301,0,0" Height="33" Click="changeData"/>
        <Button Background="Gray" Content="Trainings" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="150,361,0,0" Height="33" Click="trainingCRUD"/>
        <Button Background="Gray" Content="Fitness Center" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="150,244,0,0" Height="33" Click="fitnessCenterCRUD"/>
        <Button Background="Gray" Content="Addresses" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="150,301,0,0" Height="33" Click="addressesCRUD"/>
        <Button Background="Gray" Content="Instructors" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="533,361,0,0" Height="33" Click="instructorCRUD"/>
        <Button Background="Gray" Content="Begginers" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="533,301,0,0" Height="33" Click="beginnerCRUD"/>
        <Button Background="Gray" Content="Search" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="533,244,0,0" Height="33" Click="searchButton"/>

    </Grid>
</Window>
