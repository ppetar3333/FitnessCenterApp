<Window x:Class="fitnessCenterProject.Windows.Administrator.SearchAdmin.ShowSearchingResultBeginner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fitnessCenterProject.Windows.Administrator.SearchAdmin"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Background="LightGray"
        Title="Show Searching Result" Height="400" Width="600">
    <Grid>
        <Separator HorizontalAlignment="Left" Height="41" Margin="27,71,0,0" VerticalAlignment="Top" Width="529"/>
        <Label FontWeight="Bold" FontSize="20" Content="Result of search" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="224,34,0,0"/>
        <DataGrid AutoGeneratedColumns="changeVisibilityBeginners" Name="beginnerDataInformationDataGrid" IsReadOnly="True" AutoGenerateColumns="True" BorderBrush="Black" HorizontalAlignment="Left" Height="87" VerticalAlignment="Top" Width="484" Margin="50,164,0,0"/>
        <Button Background="Gray" Content="Close" HorizontalAlignment="Left" VerticalAlignment="Top" Width="95" Margin="250,297,0,0" Height="38" Click="closeButton"/>
        <Label FontWeight="Bold" FontSize="16" Content="Data" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="276,112,0,0" RenderTransformOrigin="1.315,0.784"/>
    </Grid>
</Window>
